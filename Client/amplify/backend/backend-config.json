{
  "storage": {
    "cherryeStorage": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    },
    "bakeproducts": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "cartitems": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "contact": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "cakeFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "bakeproducts",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "cherryea402d001CustomMessage": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "cherryea402d001PostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "handlecartitemsFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "cherryeStorage",
          "attributes": [
            "BucketName"
          ]
        },
        {
          "category": "storage",
          "resourceName": "bakeproducts",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "cartitems",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "api",
          "resourceName": "cherryeAPI",
          "attributes": [
            "ApiName",
            "ApiId"
          ]
        },
        {
          "category": "auth",
          "resourceName": "cherryea402d001",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    },
    "contactFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "cherryeStorage",
          "attributes": [
            "BucketName"
          ]
        },
        {
          "category": "storage",
          "resourceName": "contact",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "cherryea402d001CustomMessage",
          "attributes": [
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "cherryea402d001PostConfirmation",
          "attributes": [
            "Name"
          ]
        },
        {
          "category": "api",
          "resourceName": "cherryeAPI",
          "attributes": [
            "ApiName",
            "ApiId"
          ]
        },
        {
          "category": "auth",
          "resourceName": "cherryea402d001",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  },
  "api": {
    "cherryeAPI": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "cakeFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "cherryeAPICart": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "handlecartitemsFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "cherryeAPIContact": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "contactFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "auth": {
    "cherryea402d001": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "cherryea402d001CustomMessage",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "cherryea402d001PostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  }
}